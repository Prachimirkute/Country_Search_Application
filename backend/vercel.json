{
    "version": 2,
    "builds": [
      {
        "src": "index.js", 
        "use": "@vercel/node"
      }
    ],
    "routes": 
        [
            { "src": "/countries/:country", "dest": "/countries/:country" },
            { "src": "/api/countryList", "dest": "/api/countryList" },
            { "src": "/(.*)", "dest": "/" }
          
    ]
  }